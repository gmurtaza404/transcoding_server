        {{/pupEnabled}}
        {{/shouldShowResults}}

        {{#isAccordionEnabled}}

        <div class="as-retailavailabilitysearch-searchactions as-retailavailabilitysearch-selectstore">
                <button id="as-retailavailabilitysearch-storebutton" {{^storeSelectionEnabled}}disabled="disabled"{{/storeSelectionEnabled}} class="button merchandising {{^storeSelectionEnabled}}disabled{{/storeSelectionEnabled}} small-12" data-autom="selectStoreButton">
                    <span class="select-store">{{mowSelectButton}}</span>
                    <span class="a11y">
        				{{#selectedStore}}
        				{{#address}}
            				{{address}}
        				{{/address}}
        				{{/selectedStore}}
        			</span>
                </button>
        </div>

        <div class="as-retailavailabilitysearch-storedetails"></div>
        {{/isAccordionEnabled}}
        {{^isMapEnabled}}
        {{^shouldShowResults}}
            <div id="as-retailavailabilitysearch-infoplaceholder" class="as-retailavailabilitysearch-infoplaceholder large-6 small-12 row" >
                {{{personalPickup}}}
            </div>
        {{/shouldShowResults}}
        {{/isMapEnabled}}
    </div>
</div>
</script>


